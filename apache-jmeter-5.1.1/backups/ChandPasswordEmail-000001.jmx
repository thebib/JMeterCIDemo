            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.poi.xssf.usermodel.XSSFWorkbook;
import org.apache.poi.xssf.usermodel.XSSFSheet;
import org.apache.poi.xssf.usermodel.XSSFRow;
import java.io.*;
import org.apache.jmeter.gui.GuiPackage;
import org.apache.commons.io.FilenameUtils;

File excel;
FileInputStream fis;
XSSFWorkbook book;
XSSFSheet sheet;
XSSFRow row;
String petId;
int i = Integer.parseInt(vars.get(&quot;counter&quot;));
String testPlanFile = GuiPackage.getInstance().getTestPlanFile();
String testPlanFileDir = FilenameUtils.getFullPathNoEndSeparator(testPlanFile);
log.info(&quot;testPlanFileDir: &quot; + testPlanFileDir);

try {
	excel = new File(testPlanFileDir + &quot;${fileName}&quot;);
	fis = new FileInputStream(excel);
	book = new XSSFWorkbook(fis);
	sheet = book.getSheetAt(0);
} catch (Exception e) {
     log.info(&quot;!!!!!!!!&quot; + e.getMessage() + &quot;!!!!!!!!!!!!!!!&quot;);
}

try{
	row = sheet.getRow(i);
} catch (Exception e){
    	e.printStackTrace();
  	log.info(&quot;Error in Script&quot;, e);
   	throw e;
}

petId = row.getCell(0).toString();
int index = petId.indexOf(&quot;.&quot;);
petId = petId.substring(0, index);

vars.put(&quot;PET_ID&quot;, petId);

log.info(&quot;Pet ID is &quot; + petId)

book.close();</stringProp>
